var api = { list: "game/list", lock1: resourcePath + "/Images/game/game-1.jpg" }; function GetData() { PKLService.get({ url: api.list, OnSuccess: function (a) { buildDisplay(a.Data) } }) } function buildDisplay(a) { if (a && a.length > 0) { for (var e = "", t = 0; t < a.length; t++) { var i = a[t]; e += '<div class="col-xs-12 col-md-4 item"><a title="' + i.Name + '" href="/game/detail/' + i.Id + '"><img src="' + s3Storage + i.Image.replace("~", "") + '"/></a></div>' } $("#list-game").append(e) } } $(document).ready(function () { translate.onLoad(function () { GetData() }) });